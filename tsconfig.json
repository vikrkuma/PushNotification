{
  "compilerOptions": {
    "lib": ["es6", "dom"],
    "downlevelIteration": true,
    "target": "es5",
    "outDir": "build",
    "strict": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "noImplicitAny": true
  }
}
